{
	"extends": "config/tsconfig-webfront.json",
	"compilerOptions": {
		"baseUrl": ".",
		"strict": true,
		"disableSizeLimit": true,
		"declaration": false,
		"declarationMap": false
	},
	"paths": {
		"@/*": ["./src/*"]
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pre-build.js"],
	"exclude": ["node_modules"]
}
